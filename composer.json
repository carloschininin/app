{
  "name": "carloschininin/app",
  "description": "Utilities for applications",
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "carloschininin/util": "dev-master",
    "symfony/dependency-injection": "^5.4|^6.0",
    "symfony/http-kernel": "^5.4|^6.0",
    "symfony/form": "^5.4|^6.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^v3.1"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
