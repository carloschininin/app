{
  "name": "carloschininin/app",
  "description": "Utilities for applications",
  "type": "library",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "carloschininin/php-spreadsheet": "^0.1",
    "carloschininin/util": "^0.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.5",
    "doctrine/orm": "^3.0",
    "statickidz/php-google-translate-free": "^1.2",
    "symfony/cache-contracts": "^3.0",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/form": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/intl": "^7.0",
    "symfony/security-bundle": "^7.0",
    "symfony/translation-contracts": "^3.0",
    "twig/twig": "^3.10"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.55"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\App\\Tests\\": "tests/"
    }
  }
}
