{
  "name": "carloschininin/app",
  "description": "Utilities for applications",
  "version": "0.2.0",
  "type": "library",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Carlos Chininin",
      "email": "carloschininin@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "carloschininin/php-spreadsheet": "^0.2.0",
    "carloschininin/util": "^0.2.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.5|^3.2",
    "doctrine/orm": "^3.0",
    "statickidz/php-google-translate-free": "^1.2",
    "symfony/cache-contracts": "^3.0",
    "symfony/config": "^7.0|^8.0",
    "symfony/dependency-injection": "^7.0|^8.0",
    "symfony/form": "^7.0|^8.0",
    "symfony/http-kernel": "^7.0|^8.0",
    "symfony/intl": "^7.0|^8.0",
    "symfony/security-bundle": "^7.0|^8.0",
    "symfony/translation-contracts": "^3.0",
    "twig/twig": "^3.10"
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "phpunit/phpunit": "^12.5"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "CarlosChininin\\App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CarlosChininin\\App\\Tests\\": "tests/"
    }
  }
}
